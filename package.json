{
  "name": "action-mutability-prioritizer",
  "version": "0.1.0",
  "description": "GitHub Action that scans workflows for action mutability and prioritizes pinning work by scoring usage.",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/main.ts -o dist --minify",
    "lint": "eslint \"{src,tests}/**/*.ts\"",
    "test": "jest",
    "format": "prettier --write .",
    "ci:dist": "npm run build && git diff --exit-code dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/example/action-mutability-prioritizer.git"
  },
  "keywords": [
    "github-actions",
    "security",
    "supply-chain",
    "mutability",
    "pinning"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.0",
    "fast-glob": "^3.3.2",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5"
  }
}
